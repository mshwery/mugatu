<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tag %>
  </head>

  <body>
    <%= render 'layouts/header' %>

    <div id="main" class="wrap">
      <%= yield %>
    </div>

    <%= render 'layouts/footer' %>
    
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>
