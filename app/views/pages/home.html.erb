<section id="home">

  <h1>Mugatu</h1>

  <div class="list">
    <h3>Accounts:</h3>
    <ul>
      <% @accounts.each do |account| %>
        <li><%= link_to account.name, account %></li>
      <% end %>
    </ul>
  </div>

  <%= link_to "Sign up now!", signup_path, :class => "signup_button round" %>
  
</section>